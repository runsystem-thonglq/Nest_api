{
  "name": "next-nestjs-base",
  "version": "0.0.1",
  "description": "",
  "author": "thonglq",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && yarn copy-templates",
    "copy-templates": "mkdir -p dist/shared/email && cp -r src/shared/email/* dist/shared/email/",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "start": "nest start",
    "start:dev": "nest start --watch && yarn copy-templates",
    "start:preview": "nest start --builder webpack",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "db:create": "node ./node_modules/typeorm-extension/dist/cli/index.js db:create --codeTransformation jit -r dist/database -d dist/database/data-source.js",
    "db:drop": "yarn typeorm schema:drop",
    "db:generate": "yarn typeorm migration:generate src/database/migrations/migrations",
    "db:run": "yarn typeorm migration:run",
    "db:rollback": "yarn typeorm migration:revert",
    "db:create:run": "yarn db:create && yarn db:run",
    "db:seed": "tsx node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/data-source.ts",
    "seed": "ts-node ./src/scripts/seed.ts",
    "seed:dev": "NODE_ENV=development ts-node ./src/scripts/seed.ts",
    "seed:prod": "NODE_ENV=production node dist/./src/scripts/seed.js",
    "db:init": "yarn db:create && yarn db:generate && yarn db:run",
    "credentials:edit": "node bin/credentials-edit.js",
    "start:repl": "nest start --entryFile repl"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "0.1.7",
    "@nestjs-modules/ioredis": "2.0.2",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/bull": "0.6.1",
    "@nestjs/cache-manager": "2.0.1",
    "@nestjs/common": "10.0.4",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.4",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.4",
    "@nestjs/schedule": "4.1.1",
    "@nestjs/swagger": "7.0.12",
    "@slynova/flydrive": "1.0.3",
    "@slynova/flydrive-s3": "1.0.3",
    "@types/multer": "1.4.12",
    "axios": "1.7.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "4.10.1",
