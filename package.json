{
  "name": "base_source",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build ",
    "copy-templates": "cp -r src/shared/email/* dist/shared/email/",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "start": "nest start",
    "start:devw": "nodemon --legacy-watch -L --exec ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch ",
    "start:preview": "nest start --builder webpack",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk --loader tsx node_modules/.bin/jest --runInBand",
    "test:prepare": "NODE_ENV=test yarn db:create:run",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit --passWithNoTests",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "db:create": "tsx ./node_modules/typeorm-extension/dist/cli/index.js db:create -r dist/database -d data-source.js -f data-source.js",
    "db:drop": "yarn typeorm schema:drop",
    "db:generate": "yarn typeorm migration:generate src/database/migrations",
    "db:run": "yarn typeorm migration:run",
    "db:rollback": "yarn typeorm migration:revert",
    "db:create:run": "yarn db:create && yarn db:run",
    "db:seed": "tsx ./node_modules/typeorm-extension/dist/cli/index.js seed -r src/database -d data-source.ts -f data-source.ts",
    "db:init": "yarn db:create && yarn db:generate && yarn db:run",
    "credentials:edit": "node bin/credentials-edit.js",
    "start:repl": "nest start --entryFile repl"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "0.1.7",
    "@google/generative-ai": "^0.24.1",
    "@nestjs-modules/ioredis": "2.0.2",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/bull": "0.6.1",
    "@nestjs/cache-manager": "2.0.1",
    "@nestjs/common": "10.0.4",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.4",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.4",
    "@nestjs/schedule": "4.1.1",
    "@nestjs/swagger": "7.0.12",
    "@slynova/flydrive": "1.0.3",
    "@slynova/flydrive-s3": "1.0.3",
    "@types/multer": "1.4.12",
    "axios": "1.7.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "4.10.1",
    "cache-manager": "4.1.0",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "command-line-args": "5.2.1",
    "cron": "3.1.7",
    "csv-parser": "3.0.0",
    "date-holidays": "3.23.12",
    "dayjs": "1.11.6",
    "dotenv": "16.0.2",
    "exceljs": "4.4.0",
    "express-basic-auth": "1.2.1",
    "ffmpeg-static": "5.2.0",
    "fluent-ffmpeg": "2.1.3",
    "hbs": "4.2.0",
    "html-to-text": "9.0.5",
    "iconv-lite": "0.6.3",
    "ioredis": "5.4.1",
    "json5": "^2.2.3",
    "md5-file": "5.0.0",
    "mime-types": "2.1.35",
    "mock-fs": "5.2.0",
    "moji": "0.5.1",
    "ms": "2.1.3",
    "nestjs-form-data": "1.8.3",
    "nestjs-i18n": "10.2.6",
    "node-credentials": "1.5.0",
    "nodemailer": "6.9.9",
    "nodemon": "^3.1.10",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "sharp": "0.33.5",
    "typeorm": "^0.3.24",
    "typeorm-naming-strategies": "4.1.0",
    "uuid": "^11.1.0",
