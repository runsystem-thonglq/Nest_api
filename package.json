    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "command-line-args": "5.2.1",
    "cron": "3.1.7",
    "csv-parser": "3.0.0",
    "dayjs": "1.11.6",
    "dotenv": "16.0.2",
    "express-basic-auth": "1.2.1",
    "fluent-ffmpeg": "2.1.3",
